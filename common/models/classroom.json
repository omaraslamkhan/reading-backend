{
  "name": "classroom",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": true
    }
  },
  "properties": {
    "isActive": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "fcmToken": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "className": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classTimings": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classDuration": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classSubject": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "location": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "numberOfSeats": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "days": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "teacherId": {
      "type": "string",
      "required": true,
      "default": ""
    },

    "studentIds": {
      "type": "array",
      "required": false,
      "options": {
        "unique": true
      }
    }
  }, 
  "validations": [],
  "relations": {
    "Student": {
      "type": "hasMany",
      "model": "student",
      "foreignKey": "teacherId"
    },
    "Teacher": {
      "type": "hasOne",
      "model": "teacher",
      "foreignKey": "teacherId"
    }


  },
  "acls": [],
  "methods": {}
}
