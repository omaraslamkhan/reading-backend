{
  "name": "classroom",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": true
    }
  },
  "properties": {
    "isActive": {
      "type": "boolean",
      "required": false,
      "default": true
    },
  
    "className": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classTimings": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classEndTimings": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "classDuration": {
      "type": "string",
      "required": false,
      "default": ""
    },

    "classStartDate": {
      "type": "date",
      "required": false,
      "default": ""
    },
    "classEndDate": {
      "type": "date",
      "required": false,
      "default": ""
    },
    "classSubject": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "location": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "locationId": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "numberOfSeats": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "skipDates": {
      "type": [
        
        {
          "skip":"date",
          "isActive":false
        }
      ],
      "required": false,
      "default": []
    },
    "days": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "teacherId": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "classFees": {
      "type": "number",
      "required": true,
      "default": ""
    },
    "studentIds": {
      "type": "array",
      "required": false,
      "options": {
        "unique": true
      }
    },
    "adminId": {
      "type": "string",
      "required": false,
      "options": {
        "unique": true
      }
    }
  },
  "validations": [],
  "relations": {
    "Student": {
      "type": "referencesMany",
      "model": "student",
      "foreignKey": "studentIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    },

    "Teacher": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "teacherId"
    },
    "Parent": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "parentId"
    }
  },
  "acls": [],
  "methods": {}
}
